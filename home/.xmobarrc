   Config { font = "-misc-fixed-*-*-*-*-10-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , lowerOnStart = True
       , commands = [ Run Date "%e[%H:%M]" "date" 60
              	    , Run Com "ncmpcpp --now-playing" [] "mpd" 10
		    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% } %mpd% { <fc=grey>%date%</fc>"
      }
